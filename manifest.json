{
"manifest_version": 2,
"name": "Schedule Buddy",
"version": "2",
"icons": {
    "16": "images/developers-logo.png",
    "48": "images/developers-logo.png",
    "128": "images/developers-logo.png"
},
"permissions": [
    "background",
    "identity",
    "notifications",
    "storage",
    "alarms",
    "https://www.googleapis.com/*",
    "https://*.googleusercontent.com/*",
    "https://apis.google.com/js/*"
],
"content_security_policy": "script-src 'self' https://apis.google.com; object-src 'self'",
"background": {
    "scripts": [
        "background.js",
        "options.js",
        "optionsP2.js"
    ],
    "persistent": false
},


"options_ui": {
    "page": "options.html",
    
    "open_in_tab": false
},


"browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
        "16": "images/get_started16.png",
        "32": "images/get_started32.png",
        "48": "images/get_started48.png",
        "128": "images/get_started128.png"
      },
    "default_title": "Click to Sign in with Google"
},

"key": "",
"oauth2": {
    "client_id": "963870381584-r0s95shjcv94r0dk41cbdc84rn5rk8pg.apps.googleusercontent.com",
    "scopes": [
        "profile",
        "calendar",
        "https://www.googleapis.com/auth/calendar"
    ]
}
}
